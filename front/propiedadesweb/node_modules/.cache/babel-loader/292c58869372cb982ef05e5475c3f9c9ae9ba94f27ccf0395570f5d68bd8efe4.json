{"ast":null,"code":"import Paginator from \"primevue/paginator\";\nimport InmuebleService from \"../services/InmuebleService\";\nimport defaultCasaImage from '@/assets/default-house.jpg';\nimport defaultDepartamentoImage from '@/assets/default-departamento.jpg';\nimport defaultTerrenoImage from '@/assets/default-terreno.jpg';\nexport default {\n  components: {\n    Paginator\n  },\n  data() {\n    return {\n      inmuebles: [],\n      rows: 10,\n      totalRecords: 0,\n      first: 0\n    };\n  },\n  computed: {\n    paginatedInmuebles() {\n      const start = this.first;\n      const end = start + this.rows;\n      return this.inmuebles.slice(start, end);\n    }\n  },\n  created() {\n    this.inmuebleService = new InmuebleService();\n    this.loadInmuebles();\n  },\n  methods: {\n    loadInmuebles() {\n      this.inmuebleService.getAll().then(response => {\n        console.log(\"Inmuebles recibidos:\", response.data);\n        this.inmuebles = response.data;\n        this.totalRecords = response.data.length;\n        this.first = 0;\n      }).catch(error => {\n        console.error(\"Error fetching inmuebles:\", error);\n      });\n    },\n    onPageChange(event) {\n      console.log(\"Page changed:\", event);\n      this.first = event.first;\n      this.rows = event.rows;\n    },\n    formatPrice(price) {\n      return new Intl.NumberFormat('es-CL', {\n        style: 'currency',\n        currency: 'CLP'\n      }).format(price);\n    },\n    getDefaultImage(tipoInmueble) {\n      switch (tipoInmueble) {\n        case 'CASA':\n          return defaultCasaImage;\n        case 'DEPARTAMENTO':\n          return defaultDepartamentoImage;\n        case 'TERRENO':\n          return defaultTerrenoImage;\n        default:\n          return defaultCasaImage;\n      }\n    }\n  }\n};","map":{"version":3,"names":["Paginator","InmuebleService","defaultCasaImage","defaultDepartamentoImage","defaultTerrenoImage","components","data","inmuebles","rows","totalRecords","first","computed","paginatedInmuebles","start","end","slice","created","inmuebleService","loadInmuebles","methods","getAll","then","response","console","log","length","catch","error","onPageChange","event","formatPrice","price","Intl","NumberFormat","style","currency","format","getDefaultImage","tipoInmueble"],"sources":["/home/reii23/Usach/proyectoFingeso/front/propiedadesweb/src/views/Inmuebles.vue"],"sourcesContent":["<!-- TO DO:\n - mejorar el diseÃ±o de la carta de cada uno de los inmuebles\n\n-->\n\n\n\n<template>\n  <div class=\"inmuebles-container\">\n    <Paginator\n      :rows=\"rows\"\n      :totalRecords=\"totalRecords\"\n      :rowsPerPageOptions=\"[10,20,30]\"\n      @page=\"onPageChange\"\n      :first=\"first\"\n      class=\"mb-4\"\n    />\n    <div v-if=\"inmuebles.length === 0\" class=\"no-inmuebles\">No hay inmuebles disponibles.</div>\n    <div v-else class=\"inmuebles-grid\">\n      <div v-for=\"inmueble in paginatedInmuebles\" :key=\"inmueble.id\" class=\"inmueble-card\">\n        <img :src=\"inmueble.imagen || getDefaultImage(inmueble.tipoInmueble)\" :alt=\"inmueble.direccion\" class=\"inmueble-image\">\n        <div class=\"inmueble-details\">\n          <h3 class=\"inmueble-price\">{{ formatPrice(inmueble.precio) }}</h3>\n          <p class=\"inmueble-address\">{{ inmueble.direccion }}</p>\n          <div class=\"inmueble-features\">\n            <span><i class=\"pi pi-check-circle\"></i> {{ inmueble.disponible }} </span>\n            <span><i class=\"pi pi-verified\"></i> {{ inmueble.verificado }} </span>\n            <span><i class=\"pi pi-chart-bar\"></i> {{ otro_valor }} </span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Paginator from \"primevue/paginator\";\nimport InmuebleService from \"../services/InmuebleService\";\nimport defaultCasaImage from '@/assets/default-house.jpg';\nimport defaultDepartamentoImage from '@/assets/default-departamento.jpg';\nimport defaultTerrenoImage from '@/assets/default-terreno.jpg';\n\nexport default {\n  components: {\n    Paginator,\n  },\n  data() {\n    return {\n      inmuebles: [],\n      rows: 10,\n      totalRecords: 0,\n      first: 0,\n    };\n  },\n  computed: {\n    paginatedInmuebles() {\n      const start = this.first;\n      const end = start + this.rows;\n      return this.inmuebles.slice(start, end);\n    },\n  },\n  created() {\n    this.inmuebleService = new InmuebleService();\n    this.loadInmuebles();\n  },\n  methods: {\n    loadInmuebles() {\n      this.inmuebleService.getAll().then((response) => {\n        console.log(\"Inmuebles recibidos:\", response.data);\n        this.inmuebles = response.data;\n        this.totalRecords = response.data.length;\n        this.first = 0;\n      }).catch(error => {\n        console.error(\"Error fetching inmuebles:\", error);\n      });\n    },\n    onPageChange(event) {\n      console.log(\"Page changed:\", event);\n      this.first = event.first;\n      this.rows = event.rows;\n    },\n    formatPrice(price) {\n      return new Intl.NumberFormat('es-CL', { style: 'currency', currency: 'CLP' }).format(price);\n      \n    }\n    ,getDefaultImage(tipoInmueble) {\n  switch (tipoInmueble) {\n    case 'CASA':\n      return defaultCasaImage;\n    case 'DEPARTAMENTO':\n      return defaultDepartamentoImage;\n    case 'TERRENO':\n      return defaultTerrenoImage;\n    default:\n      return defaultCasaImage; \n  }\n},\n  },\n};\n</script>\n\n<style scoped>\n.inmuebles-container {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.inmuebles-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.inmueble-card {\n  border: 1px solid #111111;\n  border-radius: 8px;\n  overflow: hidden;\n  transition: box-shadow 0.3s ease;\n}\n\n.inmueble-card:hover {\n  box-shadow: 0 4px 8px rgba(0,1,1,1.1);\n}\n\n.inmueble-image {\n  width: 100%;\n  height: 200px;\n  object-fit: cover;\n}\n\n.inmueble-details {\n  padding: 15px;\n}\n\n.inmueble-price {\n  font-size: 1.4em;\n  font-weight: bold;\n  color: #07e90f;\n  margin: 0 0 10px 0;\n}\n\n.inmueble-address {\n  font-size: 1em;\n  color: #f8f5f5;\n  margin: 0 0 10px 0;\n}\n\n.inmueble-features {\n  display: flex;\n  justify-content: space-between;\n  font-size: 0.9em;\n  color: #f5f5f5;\n}\n\n.inmueble-features span {\n  display: flex;\n  align-items: center;\n}\n\n.inmueble-features i {\n  margin-right: 5px;\n}\n\n.no-inmuebles {\n  text-align: center;\n  font-size: 1.2em;\n  color: #808080;\n  margin-top: 20px;\n}\n\n@media (max-width: 768px) {\n  .inmuebles-grid {\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\n  }\n}\n</style>"],"mappings":"AAoCA,OAAOA,SAAQ,MAAO,oBAAoB;AAC1C,OAAOC,eAAc,MAAO,6BAA6B;AACzD,OAAOC,gBAAe,MAAO,4BAA4B;AACzD,OAAOC,wBAAuB,MAAO,mCAAmC;AACxE,OAAOC,mBAAkB,MAAO,8BAA8B;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE,CAAC;MACfC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAI,IAAI,CAACH,KAAK;MACxB,MAAMI,GAAE,GAAID,KAAI,GAAI,IAAI,CAACL,IAAI;MAC7B,OAAO,IAAI,CAACD,SAAS,CAACQ,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IACzC;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAc,GAAI,IAAIhB,eAAe,CAAC,CAAC;IAC5C,IAAI,CAACiB,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACPD,aAAaA,CAAA,EAAG;MACd,IAAI,CAACD,eAAe,CAACG,MAAM,CAAC,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QAC/CC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,QAAQ,CAAChB,IAAI,CAAC;QAClD,IAAI,CAACC,SAAQ,GAAIe,QAAQ,CAAChB,IAAI;QAC9B,IAAI,CAACG,YAAW,GAAIa,QAAQ,CAAChB,IAAI,CAACmB,MAAM;QACxC,IAAI,CAACf,KAAI,GAAI,CAAC;MAChB,CAAC,CAAC,CAACgB,KAAK,CAACC,KAAI,IAAK;QAChBJ,OAAO,CAACI,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;MACnD,CAAC,CAAC;IACJ,CAAC;IACDC,YAAYA,CAACC,KAAK,EAAE;MAClBN,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEK,KAAK,CAAC;MACnC,IAAI,CAACnB,KAAI,GAAImB,KAAK,CAACnB,KAAK;MACxB,IAAI,CAACF,IAAG,GAAIqB,KAAK,CAACrB,IAAI;IACxB,CAAC;IACDsB,WAAWA,CAACC,KAAK,EAAE;MAC