{"ast":null,"code":"import Paginator from \"primevue/paginator\";\nimport InmuebleService from \"../services/InmuebleService\";\nexport default {\n  components: {\n    Paginator\n  },\n  data() {\n    return {\n      inmuebles: [],\n      rows: 10,\n      totalRecords: 0,\n      first: 0\n    };\n  },\n  computed: {\n    paginatedInmuebles() {\n      const start = this.first;\n      const end = start + this.rows;\n      return this.inmuebles.slice(start, end);\n    }\n  },\n  created() {\n    this.inmuebleService = new InmuebleService();\n    this.loadInmuebles();\n  },\n  methods: {\n    loadInmuebles() {\n      this.inmuebleService.getAll().then(response => {\n        console.log(\"Inmuebles recibidos:\", response.data);\n        this.inmuebles = response.data;\n        this.totalRecords = response.data.length;\n        this.first = 0;\n      }).catch(error => {\n        console.error(\"Error fetching inmuebles:\", error);\n      });\n    },\n    onPageChange(event) {\n      console.log(\"Page changed:\", event);\n      this.first = event.first;\n      this.rows = event.rows;\n    },\n    formatPrice(price) {\n      return new Intl.NumberFormat('es-CL', {\n        style: 'currency',\n        currency: 'CLP'\n      }).format(price);\n    }\n  }\n};","map":{"version":3,"names":["Paginator","InmuebleService","components","data","inmuebles","rows","totalRecords","first","computed","paginatedInmuebles","start","end","slice","created","inmuebleService","loadInmuebles","methods","getAll","then","response","console","log","length","catch","error","onPageChange","event","formatPrice","price","Intl","NumberFormat","style","currency","format"],"sources":["/home/reii23/Usach/proyectoFingeso/front/propiedadesweb/src/views/Inmuebles.vue"],"sourcesContent":["<template>\n  <div class=\"inmuebles-container\">\n    <Paginator\n      :rows=\"rows\"\n      :totalRecords=\"totalRecords\"\n      :rowsPerPageOptions=\"[10, 20, 30]\"\n      @page=\"onPageChange\"\n      :first=\"first\"\n      class=\"mb-4\"\n    />\n    <div v-if=\"inmuebles.length === 0\" class=\"no-inmuebles\">No hay inmuebles disponibles.</div>\n    <div v-else class=\"inmuebles-grid\">\n      <div v-for=\"inmueble in paginatedInmuebles\" :key=\"inmueble.id\" class=\"inmueble-card\">\n        <img :src=\"inmueble.imagen || '/placeholder-image.jpg'\" :alt=\"inmueble.direccion\" class=\"inmueble-image\">\n        <div class=\"inmueble-details\">\n          <h3 class=\"inmueble-price\">{{ formatPrice(inmueble.precio) }}</h3>\n          <p class=\"inmueble-address\">{{ inmueble.direccion }}</p>\n          <div class=\"inmueble-features\">\n            <span><i class=\"pi pi-home\"></i> {{ inmueble.habitaciones }} hab</span>\n            <span><i class=\"pi pi-th-large\"></i> {{ inmueble.banos }} baños</span>\n            <span><i class=\"pi pi-chart-bar\"></i> {{ inmueble.metrosCuadrados }} m²</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Paginator from \"primevue/paginator\";\nimport InmuebleService from \"../services/InmuebleService\";\n\nexport default {\n  components: {\n    Paginator,\n  },\n  data() {\n    return {\n      inmuebles: [],\n      rows: 10,\n      totalRecords: 0,\n      first: 0,\n    };\n  },\n  computed: {\n    paginatedInmuebles() {\n      const start = this.first;\n      const end = start + this.rows;\n      return this.inmuebles.slice(start, end);\n    },\n  },\n  created() {\n    this.inmuebleService = new InmuebleService();\n    this.loadInmuebles();\n  },\n  methods: {\n    loadInmuebles() {\n      this.inmuebleService.getAll().then((response) => {\n        console.log(\"Inmuebles recibidos:\", response.data);\n        this.inmuebles = response.data;\n        this.totalRecords = response.data.length;\n        this.first = 0;\n      }).catch(error => {\n        console.error(\"Error fetching inmuebles:\", error);\n      });\n    },\n    onPageChange(event) {\n      console.log(\"Page changed:\", event);\n      this.first = event.first;\n      this.rows = event.rows;\n    },\n    formatPrice(price) {\n      return new Intl.NumberFormat('es-CL', { style: 'currency', currency: 'CLP' }).format(price);\n    }\n  },\n};\n</script>\n\n<style scoped>\n.inmuebles-container {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.inmuebles-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.inmueble-card {\n  border: 1px solid #e0e0e0;\n  border-radius: 8px;\n  overflow: hidden;\n  transition: box-shadow 0.3s ease;\n}\n\n.inmueble-card:hover {\n  box-shadow: 0 4px 8px rgba(0,0,0,0.1);\n}\n\n.inmueble-image {\n  width: 100%;\n  height: 200px;\n  object-fit: cover;\n}\n\n.inmueble-details {\n  padding: 15px;\n}\n\n.inmueble-price {\n  font-size: 1.4em;\n  font-weight: bold;\n  color: #4CAF50;\n  margin: 0 0 10px 0;\n}\n\n.inmueble-address {\n  font-size: 1em;\n  color: #333;\n  margin: 0 0 10px 0;\n}\n\n.inmueble-features {\n  display: flex;\n  justify-content: space-between;\n  font-size: 0.9em;\n  color: #666;\n}\n\n.inmueble-features span {\n  display: flex;\n  align-items: center;\n}\n\n.inmueble-features i {\n  margin-right: 5px;\n}\n\n.no-inmuebles {\n  text-align: center;\n  font-size: 1.2em;\n  color: #666;\n  margin-top: 20px;\n}\n\n/* Responsive design */\n@media (max-width: 768px) {\n  .inmuebles-grid {\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\n  }\n}\n</style>"],"mappings":"AA6BA,OAAOA,SAAQ,MAAO,oBAAoB;AAC1C,OAAOC,eAAc,MAAO,6BAA6B;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE,CAAC;MACfC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAI,IAAI,CAACH,KAAK;MACxB,MAAMI,GAAE,GAAID,KAAI,GAAI,IAAI,CAACL,IAAI;MAC7B,OAAO,IAAI,CAACD,SAAS,CAACQ,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IACzC;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAc,GAAI,IAAIb,